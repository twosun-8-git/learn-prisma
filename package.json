{
  "name": "coffee-beans-shop-prisma",
  "version": "1.0.0",
  "description": "Prisma学習用のコーヒー豆ECサイト",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "test:connection": "ts-node src/index.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "prisma": "^5.19.1"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
